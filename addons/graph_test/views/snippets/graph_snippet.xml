<odoo>
    <data>
        <template id="graph_snippet_mini" inherit_id="website.snippets" name="graph mini" priority="8">
            <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
                <t t-snippet="graph_test.graph_snippet_content">
                    <keywords>Snippet</keywords>
                </t>

            </xpath>
        </template>

        <template id="graph_snippet_content" name="graph snippet">
            <section>
            <div id="cualquiera">
                        <t t-call="graph_test.barchart" >
            </t>
            </div>

            </section>
        </template>
    </data>
</odoo>
